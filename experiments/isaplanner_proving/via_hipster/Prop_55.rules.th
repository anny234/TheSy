(=> "(twoSpec y y) => (Z)" (-2 ?y ?y) (Z))
(=> "(twoSpec y (S y)) => (Z)" (-2 ?y (S ?y)) (Z))
(=> "(twoSpec (S y) y) => (S Z)" (-2 (S ?y) ?y) (S Z))
(=> "(twoSpec (len x2) z) => (len (Prop_55.drop z x2))" (-2 (len ?x2) ?z) (len (drop ?z ?x2)))
(=> "(twoSpec (len x2) z) <= (len (Prop_55.drop z x2))" (len (drop ?z ?x2)) (-2 (len ?x2) ?z))
(=> "(x (x z x2) x3) => (x z (x x2 x3))" (++ (++ ?z ?x2) ?x3) (++ ?z (++ ?x2 ?x3)))
(=> "(x (x z x2) x3) <= (x z (x x2 x3))" (++ ?z (++ ?x2 ?x3)) (++ (++ ?z ?x2) ?x3))
(=> "(twoSpec y (S (S y))) => (Z)" (-2 ?y (S (S ?y))) (Z))
(=> "(twoSpec (S (S y)) y) => (S (S Z))" (-2 (S (S ?y)) ?y) (S (S Z)))
