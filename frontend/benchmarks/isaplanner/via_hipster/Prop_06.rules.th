(=> "(twoSpec (twoSpec x y) z) => (twoSpec x (twoSpectwoSpec y z))" (+2 (+2 ?x ?y) ?z) (+2 ?x (|-2| ?y ?z)))
(=> "(twoSpec (twoSpec x y) z) <= (twoSpec x (twoSpectwoSpec y z))" (+2 ?x (|-2| ?y ?z)) (+2 (+2 ?x ?y) ?z))
(=> "(twoSpec (twoSpectwoSpec y x) y) => (twoSpec x (twoSpec y y))" (+2 (|-2| ?y ?x) ?y) (+2 ?x (+2 ?y ?y)))
(=> "(twoSpec (twoSpectwoSpec y x) y) <= (twoSpec x (twoSpec y y))" (+2 ?x (+2 ?y ?y)) (+2 (|-2| ?y ?x) ?y))
(=> "(twoSpec (twoSpectwoSpec y x) x) => (y)" (+2 (|-2| ?y ?x) ?x) (?y))
(=> "(twoSpec (twoSpectwoSpec y x) x) <= (y)" (?y) (+2 (|-2| ?y ?x) ?x))
