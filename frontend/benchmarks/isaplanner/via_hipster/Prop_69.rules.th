(=> "(twoSpec x x)" (<=2 ?x ?x) true)
(=> "(twoSpec x (S x))" (<=2 ?x (S ?x)) true)
(=> "(not (twoSpec (S x) x))" (not (<=2 (S ?x) ?x)) true)
(=> "[(=> (twoSpec y x) (not (twoSpec (S x) y)))]" (=> (<=2 ?y ?x) (= (not (<=2 (S ?x) ?y)) true)))
(=> "[(=> (twoSpec x y) (twoSpec x (S y)))]" (=> (<=2 ?x ?y) (= (<=2 ?x (S ?y)) true)))
(=> "(twoSpec x (twoSpectwoSpec x y))" (<=2 ?x (+2 ?x ?y)) true)
(=> "(twoSpec y (twoSpectwoSpec x y))" (<=2 ?y (+2 ?x ?y)) true)
